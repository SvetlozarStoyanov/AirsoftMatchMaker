@model IEnumerable<RoleListModel>
@{
    ViewBag.Title = "Roles";
}
<partial name="_UserRoles" model="@ViewBag.UserRoles"></partial>
@if (Model == null)
{
    <partial name="_RequestedRoles" model="@ViewBag.RequestedRoles"></partial>
}
else
{
    <partial name="_RequestableRoles" model="@Model"></partial>
}
